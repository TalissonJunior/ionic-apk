{
  "name": "ionic-apk",
  "version": "1.0.0",
  "description": "Ionic apk generator",
  "preferGlobal": true,
  "bin": "./dist/ionic.apk.js",
  "main": "electron.js",
  "scripts": {
    "debug": "node --inspect-brk ./dist/ionic.apk.js",
    "watch": "node node_modules/webpack/bin/webpack.js --config ./webpack.bundle.config.js && node node_modules/webpack/bin/webpack.js --watch",
    "build": "node node_modules/webpack/bin/webpack.js --config ./webpack.bundle.config.js && node node_modules/webpack/bin/webpack.js",
    "electron": "node node_modules/electron/cli.js ."
  },
  "keywords": [
    "ionic",
    "apk",
    "generator"
  ],
  "author": "Talisson Junior",
  "license": "MIT",
  "devDependencies": {
    "copy-webpack-plugin": "^5.0.1",
    "css-loader": "^2.1.1",
    "electron": "^4.0.8",
    "raw-loader": "^1.0.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.14.0",
    "tslint-webpack-plugin": "^2.0.4",
    "typescript": "^3.3.3333",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.7"
  }
}
